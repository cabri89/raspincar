<!DOCTYPE HTML>
<html>
<head>
   <meta name="viewport" content="width=device-width, initial-scale=1.0"/>
   <title>Car | {% block title %}{% endblock %}</title>
   <link href="https://fonts.googleapis.com/icon?family=Material+Icons" rel="stylesheet">
   <script type="text/javascript" src="https://code.jquery.com/jquery-2.1.1.min.js"></script>
   <link href="{{ asset('css/materialize.min.css') }}" rel="stylesheet">
   <link href="{{ asset('css/custom.css') }}" rel="stylesheet">
   <script src="{{ asset('js/Chart.min.js') }}"></script>
   {% block head %}

   {% endblock %}
</head>
<body>
  <nav>
    <div class="nav-wrapper">
      <a href="{{ path('app_account_index') }}" class="brand-logo left">Raspincar</a>
      <ul class="right">
         {% if is_granted('IS_AUTHENTICATED_FULLY') %}
           {% if is_granted('ROLE_ADMIN') %}
             <li role="listuser">
               <a href="{{ path('app_adminuser_index') }}">Accueil</a>
             </li>
             <li role="listuser">
               <a href="{{ path('app_adminuser_listuser') }}">Liste des utilisateurs</a>
             </li>
           {% endif %}
         <li role="presentation">
            <a href="{{ path('logout') }}">DÃ©connexion</a>
         </li>
         {% endif %}
         {% if not is_granted('IS_AUTHENTICATED_FULLY') %}
         <li role="presentation" class="">
            <a href="{{ path('app_security_login') }}">Connexion</a>
         </li>
         <li role="presentation" class="">
            <a href="{{ path('app_registration_register') }}">Inscription</a>
         </li>
      </ul>

   </div>
         {% endif %}

  </nav>
  <div class="container">
        <!-- Page Content goes here -->
         {% block body %}
         {% endblock %}
     </div>

         <script src="{{ asset('js/materialize.min.js') }}"></script>
</body>
</html>
